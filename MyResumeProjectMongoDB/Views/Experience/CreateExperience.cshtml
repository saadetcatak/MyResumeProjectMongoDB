@model Experience

@{
    ViewData["Title"] = "CreateExperience";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<div class="container">
    <form method="post">
        <label>Deneyim Başlık</label>
        <input type="text" asp-for="Title" class="form-control" />
        <br />
        <label>Şirket</label>
        <input type="text" asp-for="Company" class="form-control" />
        <br />
        <label>Tarih</label>
        <input type="text" asp-for="Time" class="form-control" />
        <br />
        <button class="btn btn-outline-success"><i class="fas fa-save"></i>Ekle</button>
    </form>
</div>